{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/app.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/app.ts",
    "start": "node dist/app.js",
    "start:flyio": "ts-node -r tsconfig-paths/register src/app.ts",
    "run-script": "ts-node -r tsconfig-paths/register src/scripts/run-script-manual.ts",
    "build": "tsc && tsc-alias",
    "build:watch": "concurrently --kill-others \"tsc --build -w\" \"tsc-alias -w\"",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "create-pods": "ts-node -r tsconfig-paths/register src/scripts/create-pods.ts",
    "create-proposal": "ts-node -r tsconfig-paths/register src/scripts/create-proposal.ts",
    "create-value-questions": "ts-node -r tsconfig-paths/register src/scripts/create-value-questions.ts",
    "edit-space": "ts-node -r tsconfig-paths/register src/scripts/edit-space.ts",
    "export-data": "ts-node -r tsconfig-paths/register src/scripts/export-data.ts",
    "export-votes": "ts-node -r tsconfig-paths/register src/scripts/export-votes.ts",
    "export-new-users": "ts-node -r tsconfig-paths/register src/scripts/export-new-users.ts",
    "export-by-voters-phase": "ts-node -r tsconfig-paths/register src/scripts/export-by-voters-phase.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/wallet": "^5.7.0",
