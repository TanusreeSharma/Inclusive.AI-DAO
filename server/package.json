    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "chalk": "4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.40.0",
    "compression": "^1.7.4",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.3.1",
    "es6-shim": "^0.35.8",
    "eslint-config-prettier": "^8.10.0",
    "ethers": "^6.7.1",
    "express": "^4.18.2",
    "finalhandler": "^1.2.0",
    "graphql": "^16.8.0",
    "http-status": "^1.6.2",
    "husky": "^8.0.3",
    "identicon": "^3.1.1",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jose": "^4.14.4",
    "jsonwebtoken": "^9.0.1",
    "jwks-rsa": "^3.0.1",
