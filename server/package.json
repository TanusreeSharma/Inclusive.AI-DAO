    "pg": "^8.11.2",
    "redis": "^3.1.2",
    "routing-controllers": "^0.10.4",
    "socket-controllers": "^0.2.0",
    "socket.io": "^4.7.2",
    "streamifier": "^0.1.1",
    "ts-node": "^10.9.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.17",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.3",
    "@json2csv/node": "^7.0.3",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/passport": "^1.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
